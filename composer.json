{
  "name": "szemul/router",
  "description": "Router interface for szemul/framework",
  "license": "MIT",
  "minimum-stability": "stable",
  "require": {
    "php": "^8.1",
    "slim/slim": "^4.8"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.0.0",
    "jetbrains/phpstorm-attributes": "^1.0",
    "mockery/mockery": "^1.0",
    "phpstan/phpstan": "^1.4.0",
    "phpunit/phpunit": "^9.5.5"
  },
  "autoload": {
    "psr-4": {
      "Szemul\\Router\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Szemul\\Router\\Test\\": "tests"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "allow-plugins": false
  }
}
